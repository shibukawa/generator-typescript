{
  "name": <%- JSON.stringify(slugname) %>,
  "private": true,
  "version": "0.0.1",
  "description": <%- JSON.stringify(description) %>,
  "scripts": {
    "build": "npm-run-all -p lib:build doc:update lib:copy1 lib:copy2  lib:copy3",
        "lib:build": "tsc -p projects/<%- slugname %>",
        "lib:copy1": "cpx \"projects/<%- slugname %>/package.json\" \"dist/<%- slugname %>\"",
        "lib:copy2": "cpx \"projects/<%- slugname %>/*.md\" \"dist/<%- slugname %>\"",
        "lib:copy3": "cpx \"projects/<%- slugname %>/.npmignore\" \"dist/<%- slugname %>\"",
        "doc:update": "react-sample-page-generator -p projects/<%- slugname %>",
    "dev": "next",
    "export": "npm-run-all -s build doc:build doc:export",
        "doc:build": "next build",
        "doc:export": "next export",
        "doc:server": "http-server out -o",
    "prod": "npm-run-all -s export doc:server",
    "lint": "tslint --project .",
    "fix": "tslint --fix --project .",
    "test": "jest",
    "watch": "jest --watchAll",
    "prepack": "npm run build",
    "pack": "cd dist/<%- slugname %> && npm pack"
  },
  "keywords": [],
  "author": {
    "name": <%- JSON.stringify(authorName) %>,
    "email": <%- JSON.stringify(authorEmail) %><% if (authorUrl) { %>,
    "url": <%- JSON.stringify(authorUrl) %><% } %>
  },
  "license": <%- JSON.stringify(license) %>,
  "repository": <%- JSON.stringify(repoUrl) %>,
  "devDependencies": {
    "@types/jest": "^23.3.3",
    "@types/node": "^10.11.4",
    "@types/react": "^16.4.14",
    "@types/react-dom": "^16.0.8",
    "@zeit/next-typescript": "^1.1.1",
    "@zeit/next-sass": "^1.0.1",
    "@zeit/next-source-maps": "0.0.3",
    "cpx": "^1.5.0",
    "jest": "^23.6.0",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.14.3",
    "puppeteer": "^1.8.0",
    "ts-jest": "^23.10.3",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-plugin-prettier": "^2.0.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.1.1",
    "react-sample-page-generator": "^0.1.0"
  },
  "dependencies": {
    "@material-ui/core": "^3.2.0",
    "@material-ui/icons": "^3.0.1",
    "next": "^7.0.1",
    "jss": "^9.8.7",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-jss": "^8.6.1",
    "react-markdown": "^4.0.1",
    "react-syntax-highlighter": "^9.0.1"
  }
}
